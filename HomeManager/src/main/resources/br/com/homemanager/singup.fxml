<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Bloom?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="485.0" prefWidth="715.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.homemanager.controller.SingupController">
    <children>
        <AnchorPane layoutX="316.0" prefHeight="487.0" prefWidth="400.0" styleClass="right_pane" stylesheets="@styles/singupStyle.css">
            <children>
                <Label layoutX="40.0" layoutY="269.0" styleClass="label-styled" stylesheets="@styles/singupStyle.css" text="número de membros" textFill="#345449">
                    <font>
                        <Font name="DejaVu Sans Condensed Bold" size="15.0" />
                    </font>
                </Label>
                <Label layoutX="38.0" layoutY="189.0" styleClass="label-styled" stylesheets="@styles/singupStyle.css" text="senha" textFill="#345449">
                    <font>
                        <Font name="DejaVu Sans Condensed Bold" size="15.0" />
                    </font>
                </Label>
                <Label layoutX="36.0" layoutY="118.0" styleClass="label-styled" stylesheets="@styles/singupStyle.css" text="username" textFill="#345449">
                    <font>
                        <Font name="DejaVu Sans Condensed Bold" size="15.0" />
                    </font>
                </Label>
                <TextField fx:id="txtUsername" layoutX="38.0" layoutY="144.0" onKeyReleased="#onKeyReleased" prefHeight="36.0" prefWidth="317.0" promptText="username da casa" styleClass="text-field-no-border">
                    <effect>
                        <Glow />
                    </effect>
                </TextField>
                <ComboBox fx:id="cboQuantidadeMembros" layoutX="229.0" layoutY="268.0" onKeyReleased="#onKeyReleased" prefHeight="19.0" prefWidth="41.0">
                    <effect>
                        <Bloom />
                    </effect>
                </ComboBox>
                <Label layoutX="85.0" layoutY="55.0" prefHeight="39.0" prefWidth="245.0" text="Cadastre sua casa" textFill="#345449">
                    <font>
                        <Font name="DejaVu Sans Condensed Bold" size="25.0" />
                    </font>
                </Label>
                <PasswordField fx:id="txtPassword" layoutX="39.0" layoutY="217.0" onKeyReleased="#onKeyReleased" prefHeight="36.0" prefWidth="317.0" promptText="senha da casa" styleClass="text-field-no-border" stylesheets="@styles/singupStyle.css">
                    <effect>
                        <Glow />
                    </effect>
                </PasswordField>
                <ScrollPane layoutX="47.0" layoutY="308.0">
                    <content>
                        <VBox fx:id="vboxNomesMembros" prefHeight="122.0" prefWidth="301.0">
                            <children>
                                <TextField disable="true" onKeyReleased="#onKeyReleased" />
                            </children>
                        </VBox>
                    </content>
                </ScrollPane>
                <Label fx:id="lbResult" alignment="CENTER" contentDisplay="CENTER" layoutX="20.0" layoutY="94.0" prefHeight="16.0" prefWidth="365.0" textAlignment="CENTER" textFill="RED" />
                <Button fx:id="btnConcluir" disable="true" layoutX="165.0" layoutY="449.0" mnemonicParsing="false" onAction="#onBtnConcluirClick" onKeyReleased="#onKeyReleased" styleClass="button-styled" stylesheets="@styles/singupStyle.css" text="Concluir" AnchorPane.bottomAnchor="12.0">
                    <font>
                        <Font name="DejaVu Sans Condensed Bold" size="13.0" />
                    </font>
                </Button>
            </children>
        </AnchorPane>
        <Pane layoutX="-3.0" layoutY="-2.0" prefHeight="487.0" prefWidth="319.0" styleClass="left_pane" stylesheets="@styles/singupStyle.css">
            <children>
                <Label layoutX="22.0" layoutY="146.0" prefHeight="123.0" prefWidth="290.0" text="&quot;A verdadeira beleza da organização está na cooperação, não na sobrecarga.&quot;" textFill="#345449" wrapText="true">
                    <font>
                        <Font name="DejaVu Sans Condensed Bold" size="21.0" />
                    </font>
                </Label>
                <Label layoutX="22.0" layoutY="261.0" prefHeight="38.0" prefWidth="271.0" text="Em média, mulheres dedicam 10,4 horas por semana a mais que os homens aos afazeres domésticos." textFill="#345449" wrapText="true">
                    <font>
                        <Font name="DejaVu Sans Condensed Bold" size="10.0" />
                    </font>
                </Label>
                <Button fx:id="btnVoltar" layoutX="22.0" layoutY="19.0" mnemonicParsing="false" onAction="#onBtnVoltarClick" onKeyReleased="#onKeyReleased" prefHeight="24.0" prefWidth="63.0" styleClass="button-styled" stylesheets="@styles/singupStyle.css" text="Voltar">
                    <font>
                        <Font name="DejaVu Sans Condensed Bold" size="13.0" />
                    </font>
                </Button>
            </children>
        </Pane>
    </children>
</AnchorPane>
